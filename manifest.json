{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "loader.js" ],
      "matches": [ "file:///*", "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "The video ad remover",
   "key": "",
   "manifest_version": 2,
   "name": "Pure motion",
   "permissions": [ "activeTab", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*", "file://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.168",
   "web_accessible_resources": [ "img/*", "templates/*", "plyr/*", "video.js/*", "videojs/*" ]
}
